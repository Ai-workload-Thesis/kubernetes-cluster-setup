# roles/admin-webapp/templates/webapp-service.yaml.j2
apiVersion: v1
kind: Service
metadata:
  name: admin-webapp-service
  namespace: {{ webapp_namespace }}
spec:
  type: NodePort
  selector:
    app: admin-webapp
  ports:
  - port: 5000
    targetPort: 5000
    nodePort: {{ webapp_port | int + 25000 }}
    protocol: TCP
